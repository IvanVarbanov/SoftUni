{{> header}}

<form method="POST" action="#/registration">

    <div>
        <label for="new-make">Email</label>
        <input id="new-make" name="email" type="text">
    </div>

    <div>
        <label for="new-make">Password</label>
        <input id="new-make" name="password" type="text">
    </div>

    <div>
        <label for="new-make">Repeat Password:</label>
        <input id="new-make" name="repeatPassword" type="text">
    </div>

    <input type="submit" class="btn btn-primary" value="Registration" />

</form>

{{> footer}}